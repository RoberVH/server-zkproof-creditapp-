#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/roberto/proyects/10 circom/zkp-validwage/server-zkproof-creditapp/node_modules/.pnpm/circom_runtime@0.1.28/node_modules/circom_runtime/node_modules:/home/roberto/proyects/10 circom/zkp-validwage/server-zkproof-creditapp/node_modules/.pnpm/circom_runtime@0.1.28/node_modules:/home/roberto/proyects/10 circom/zkp-validwage/server-zkproof-creditapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/roberto/proyects/10 circom/zkp-validwage/server-zkproof-creditapp/node_modules/.pnpm/circom_runtime@0.1.28/node_modules/circom_runtime/node_modules:/home/roberto/proyects/10 circom/zkp-validwage/server-zkproof-creditapp/node_modules/.pnpm/circom_runtime@0.1.28/node_modules:/home/roberto/proyects/10 circom/zkp-validwage/server-zkproof-creditapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../calcwit.js" "$@"
else
  exec node  "$basedir/../../calcwit.js" "$@"
fi
